<Window x:Class="SP_Threads.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SP_Threads"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="356">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Factorial"/>
            <TextBox x:Name="factorialTB" Margin="57,0,0,0"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Fibonacchi"/>
            <TextBox x:Name="fibonacchiTB" Margin="44,0,0,0"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="Simple number"/>
            <TextBox x:Name="simpleTB" Margin="20,0,0,0"/>
        </StackPanel>
    </StackPanel>
</Window>
